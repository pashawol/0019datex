mixin s-competences-2()
	// start s-competences
	+b.s-competences.section.block-with-lazy&attributes(attributes)
		.container
			//- +b.section-title
			//- 	h2(class="wow fadeInUp" data-wow-duration=`${dur * 1}s`  )  Ключевые компетенции 
			//- 		|
			//- 		span.text-primary нашей команды 
			//- 	.h4(class="wow fadeInUp" data-wow-duration=`${dur * 1}s` data-wow-delay=`${delay * .5}s`)  Автоматизируя процессы в&nbsp;вашем бизнесе, мы&nbsp;делаем его эффективнее.
		 
			mixin item(i, t,  or, d, ep) 
				+e.item
					//- .before 0
					//- 	= i
					.row
						.col-md-6.col-12.col-lg-8.col-xl-7.text-left
							.row
								.col-auto.d-none.d-xl-block
									+e.img-wrapper
										img(src="img/product-box.png" alt="")
								.col
									+e.item-title
										a(href="#")=t
									+e.descr Автоматизация процессов управления на предприятиях до&nbsp;20 мест.
									if d
										+e.demo Бесплатный демо-доступ
						//- h6= or
						.col-12.col-md-6.col-lg-4.col-xl-5
							.row
								.col-xl-7.col-6.col-md-12.d-flex.d-md-block.justify-content-center
									.row.justify-content-end
										if ep
											+e.price-col.price-light-col.col-xl-12.col-12.col-md-6.order-last.order-md-first.order-xl-last.d-flex.d-lg-block.justify-content-center
