													+e.price-wrapper.bg-light от 1 200
														span.i-font o
														|/мес.
										+e.price-col.col-xl-12.col-12.col-md-6.col-xl-12.col-6.d-flex.d-lg-block.justify-content-center
											+e.price Цена:
												+e.price-wrapper 38 500
													span.i-font o
								+e.btn-col-wrapper.col-xl-auto.col-6.col-md-12
									+e.btn-row.row.justify-content-end
										if ep
											+e.btn-col.col-xl-12.col-12.col-md-6.order-last.order-md-first.order-xl-last
												if or
													+e.btn.btn--white.btn-primary.popup-with-move-anim(href="#arend" data-title="Оставить заявку" data-order= or   data-btn="Оставить заявку") арендовать
												else
													+e.btn.btn--white.btn-primary(href="#arend" data-title="заказать" data-order="заказать"   data-btn="заказать") арендовать
										+e.btn-col.col-xl-12.col-12.col-md-6
											if or
												+e.btn.btn-primary.popup-with-move-anim(href="#order" data-title="Оставить заявку" data-order= or   data-btn="Оставить заявку") заказать
													span
											else
												+e.btn.btn-primary(href="#order" data-title="заказать" data-order="заказать"   data-btn="заказать") заказать
													span
				// /+e.item
			+item('1','1С:Бухгалтерия 8 КОРП (USB)','оптово-розничная торговля','d')
			+item('2','1С:Бухгалтерия государственного учреждения 8 ПРОФ (USB)','бухгалтерский учёт','','ep')
			+item('3','1С:Бухгалтерия 8. Комплект на 5 пользователей','учёт зарплаты и кадров')
